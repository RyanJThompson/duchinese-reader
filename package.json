{
  "name": "duchinese-reader",
  "private": true,
  "version": "0.0.0",
  "description": "A local-first lesson reader and scraper for DuChinese",
  "license": "MIT",
  "keywords": [
    "chinese",
    "language-learning",
    "duchinese",
    "reader"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "scrape": "tsx scraper/src/index.ts --output-dir public/data --no-text",
    "scrape:full": "tsx scraper/src/index.ts --output-dir scraper/output",
    "quickstart": "tsx scripts/setup.ts",
    "deploy:vercel": "npx vercel --prod --archive=tgz",
    "redeploy": "npx vercel --prod --archive=tgz"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@upstash/redis": "^1.36.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.13.0",
    "tailwindcss": "^4.1.18"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vercel/node": "^5.5.33",
    "@vitejs/plugin-react": "^5.1.1",
    "esbuild": "^0.27.3",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "tsx": "^4.19.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
